{"version": 2,
"builds": [
    {
      "src": "./api/chat.js",
      "use": "@vercel/node"
    }
  ],
"routes": [
    {
      "src": "/(.*)",
      "dest": "/api/chat.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ] 
}